---
import GenericIcon from '../GenericIcon.astro';
import type { CustomIconProps } from '../';

type Props = CustomIconProps;
const { name, title, size, width, height, ...props } = Astro.props;
---

<GenericIcon
  name={name ?? ''}
  title={title ?? ''}
  size={size}
  width={width}
  height={height}
  {...props}
/>
