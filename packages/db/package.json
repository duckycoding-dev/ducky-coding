{
  "name": "@ducky-coding/db",
  "version": "0.0.0",
  "type": "module",
  "description": "Database system for DuckyCoding.dev: mostly used for content related data, but also for user related data",
  "keywords": [
    "database",
    "drizzle",
    "sqlite",
    "turso"
  ],
  "author": "Ducky <duckycoding.dev@gmail.com>",
  "homepage": "https://github.com/duckycoding-dev/ducky-coding#readme",
  "license": "MIT",
  "main": ".dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duckycoding-dev/ducky-coding.git"
  },
  "scripts": {
    "test": "node ./__tests__/db.test.js",
    "dev": "turso dev --db-file database/content.db && tsc -w",
    "build": "tsc && npm run db:migrate",
    "db:migrate": "tsx ./src/migrate.ts",
    "drizzle:studio": "npx drizzle-kit studio",
    "drizzle:generate_migrations": "drizzle-kit generate",
    "drizzle:seed": "node --loader esbuild-register/loader -r esbuild-register ./db/seed.ts"
  },
  "bugs": {
    "url": "https://github.com/duckycoding-dev/ducky-coding/issues"
  },
  "dependencies": {
    "@ducky-coding/types": "*",
    "@libsql/client": "^0.7.0",
    "@types/jsonwebtoken": "^9.0.6",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.23.0",
    "drizzle-orm": "^0.32.0",
    "drizzle-zod": "^0.5.1",
    "jsonwebtoken": "^9.0.2"
  },
  "exports": {
    ".": "./src/index.ts",
    "./models": "./src/models/index.ts",
    "./services": "./src/services/index.ts"
  }
}
