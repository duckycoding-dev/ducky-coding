{
  "name": "@ducky-coding/astro-app",
  "type": "module",
  "version": "0.0.1",
  "description": "Package containing mainly the frontend code written in Astro JS",
  "keywords": [
    "astro"
  ],
  "author": "DuckyCoding <duckycoding.dev@gmail.com>",
  "homepage": "https://github.com/duckycoding-dev/ducky-coding#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duckycoding-dev/ducky-coding.git"
  },
  "bugs": {
    "url": "https://github.com/duckycoding-dev/ducky-coding/issues"
  },
  "main": "dist/index.html",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "astro dev",
    "db:run": "turso dev --db-file content.db",
    "db:generate_nigrations": "drizzle-kit generate:sqlite",
    "db:migrate": "tsx db/migrate.ts",
    "start": "astro dev",
    "build": "astro check && npm run db:migrate && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "drizzle:studio": "npx drizzle-kit studio",
    "drizzle:seed": "node --loader esbuild-register/loader -r esbuild-register ./db/seed.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.3",
    "@astrojs/db": "^0.12.0",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/netlify": "^5.4.0",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@ducky-coding/types": "*",
    "@ducky-coding/utils": "*",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/ph": "^1.1.13",
    "@libsql/client": "^0.7.0",
    "astro": "^4.12.2",
    "astro-icon": "^1.1.0",
    "astro-seo": "^0.8.4",
    "class-variance-authority": "^0.7.0",
    "drizzle-orm": "^0.32.0",
    "tailwindcss": "^3.4.1",
    "drizzle-kit": "^0.23.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {}
}
